{
    "name": "food",
    "bsc-flags": [
        "-bs-no-version-header",
        "-bs-super-errors"
    ],
    "reason": {
        "react-jsx": 2
    },
    "bs-dependencies": [
        "reason-react",
        "reason-apollo"
    ],
    "ppx-flags": [
        "graphql_ppx/ppx"
    ],
    "sources": [
        {
            "dir": "js",
            "subdirs": true
        }
    ],
    "package-specs": {
        "module": "commonjs",
        "in-source": true
    },
    "suffix": ".bs.js",
    "refmt": 3
}